{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "ESNext",
    "paths": {
        "*": ["src/*"],
        "@components/*": ["src/components/*"],
        "@services/*": ["src/services/*"],
        "@store/*": ["src/store/*"],
        "@utils/*": ["src/utils/*"],
        "@screens/*": ["src/screens/*"],
        "@theme/*": ["src/theme/*"],
        "@navigation/*": ["src/routes/*"],
        "@models/*": ["src/types/*"]
    },
  },
  "extends": "expo/tsconfig.base",
  "typeRoots": ["./src/types"],


}
